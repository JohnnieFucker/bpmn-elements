{
  "name": "bpmn-elements",
  "version": "8.2.2",
  "description": "Executable workflow elements based on BPMN 2.0",
  "main": "dist/index.js",
  "module": "index.js",
  "sideEffects": false,
  "scripts": {
    "test": "mocha -R dot",
    "posttest": "npm run dist && eslint . --cache",
    "prepare": "npm run dist",
    "cov:html": "nyc mocha -R dot && nyc report --reporter=html",
    "test:lcov": "nyc mocha -R dot && nyc report --reporter lcov && npm run posttest",
    "dist": "babel index.js -d dist && babel src -d dist/src",
    "predist": "node -e \"fs.rmdirSync('./dist/src', {recursive: true});\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/paed01/bpmn-elements"
  },
  "author": {
    "name": "PÃ¥l Edman",
    "url": "https://github.com/paed01"
  },
  "license": "MIT",
  "keywords": [
    "workflow",
    "automation",
    "bpmn",
    "bpmn 2",
    "isomorphic",
    "tree-shakeable"
  ],
  "nyc": {
    "exclude": [
      "dist",
      "test",
      "babel.config.js"
    ]
  },
  "files": [
    "dist/src",
    "dist/index.js",
    "src",
    "index.js"
  ],
  "devDependencies": {
    "@aircall/expression-parser": "^1.0.4",
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "bpmn-moddle": "^7.1.2",
    "camunda-bpmn-moddle": "^6.1.2",
    "chai": "^4.3.7",
    "chronokinesis": "^3.0.0",
    "debug": "^4.3.4",
    "eslint": "^8.28.0",
    "got": "^11.8.5",
    "mocha": "^10.1.0",
    "mocha-cakes-2": "^3.3.0",
    "moddle-context-serializer": "^2.1.0",
    "nock": "^13.2.8",
    "nyc": "^15.1.0"
  },
  "dependencies": {
    "iso8601-duration": "^2.1.1",
    "smqp": "^6.0.0"
  }
}
