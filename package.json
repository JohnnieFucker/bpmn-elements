{
  "name": "bpmn-elements",
  "version": "12.0.0",
  "description": "Executable workflow elements based on BPMN 2.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "src/index.js",
  "jsnext:main": "./src/index.js",
  "types": "./types",
  "exports": {
    "import": "./src/index.js",
    "require": "./dist/index.js"
  },
  "sideEffects": false,
  "scripts": {
    "test": "mocha -R @bonniernews/hot-bev -p -t 3000",
    "posttest": "npm run lint && npm run dist",
    "lint": "eslint . --cache",
    "prepack": "npm run dist",
    "cov:html": "c8 -r html -r text mocha -R @bonniernews/hot-bev -p -t 3000",
    "test:lcov": "c8 -r lcov mocha && npm run lint",
    "dist": "babel src -d dist/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/paed01/bpmn-elements"
  },
  "author": {
    "name": "PÃ¥l Edman",
    "url": "https://github.com/paed01"
  },
  "license": "MIT",
  "keywords": [
    "workflow",
    "automation",
    "bpmn",
    "bpmn 2",
    "isomorphic",
    "tree-shakeable"
  ],
  "files": [
    "dist",
    "types",
    "src"
  ],
  "devDependencies": {
    "@aircall/expression-parser": "^1.0.4",
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/register": "^7.22.5",
    "@bonniernews/hot-bev": "^0.4.0",
    "bpmn-moddle": "^8.0.1",
    "c8": "^8.0.0",
    "camunda-bpmn-moddle": "^7.0.1",
    "chai": "^4.3.7",
    "chronokinesis": "^6.0.0",
    "debug": "^4.3.4",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "got": "^12.6.1",
    "mocha": "^10.1.0",
    "mocha-cakes-2": "^3.3.0",
    "moddle-context-serializer": "^4.0.0",
    "nock": "^13.4.0"
  },
  "dependencies": {
    "smqp": "^8.0.0"
  }
}
